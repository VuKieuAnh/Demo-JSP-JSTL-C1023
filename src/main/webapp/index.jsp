<%@ page import="java.util.List" %>
<%@ page import="org.example.demo2.model.Customer" %>
<%@ page import="java.util.ArrayList" %>
<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<%@taglib prefix="ka" uri="http://java.sun.com/jsp/jstl/core" %>
<!DOCTYPE html>
<html>
<head>
    <title>JSP - Hello World</title>
</head>
<body>
<h1><%= "Hello World!" %>
</h1>
<%


    List<Customer> customers = new ArrayList<>();
    customers.add(new Customer("Huynh Tran Thanh Dung", "1/2/2000", "Da Nang", "data:image/jpeg;base64,"));
    customers.add(new Customer("Le Dang Phap", "12/4/2001", "Da Nang"));
    customers.add(new Customer("Nguyen Van Anh", "12/4/2001", "Hue"));

    request.setAttribute("khachhang", customers);

    out.print(customers.size());
%>
<table border="1px">
    <tr>
        <td>Ten</td>
        <td>Ngay sinh</td>
        <td>Dia chi</td>
        <td>Anh</td>
    </tr>
    <ka:forEach var="kh" items="${khachhang}" >
        <tr>
            <td>${kh.name}</td>
            <td>${kh.dateOfBirth}</td>
            <td>${kh.address}</td>
            <td>
                <img src="${kh.img}">
            </td>
        </tr>
    </ka:forEach>
</table>
</body>
</html>